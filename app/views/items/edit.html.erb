<h1>Editing item</h1>
<% form_for(@item) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :data_template_id %><br />
    <%= f.text_field :data_template_id %>
  </p>
  <p>
    <%= f.label :supplier_id %><br />
    <%= f.text_field :supplier_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>
<%= link_to 'Show', @item %> |
<%= link_to 'Back', items_path %>