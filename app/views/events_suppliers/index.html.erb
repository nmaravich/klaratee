<h1>Listing events_suppliers</h1>

<table>
  <tr>
    <th>Event</th>
    <th>Supplier</th>
  </tr>

<% @events_suppliers.each do |events_supplier| %>
  <tr>
    <td><%=h events_supplier.event_id %></td>
    <td><%=h events_supplier.supplier_id %></td>
    <td><%= link_to 'Show', events_supplier %></td>
    <td><%= link_to 'Edit', edit_events_supplier_path(events_supplier) %></td>
    <td><%= link_to 'Destroy', events_supplier, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New events_supplier', new_events_supplier_path %>